  "version": "0.2.0",
  "configurations": [
    {
      "type": "firefox",
      "request": "launch",
      "name": "Firefox",
      "url": "http://localhost:3000",
      "webRoot": "${workspaceFolder}",
      "pathMappings": [
        {
          "url": "file:///app/src/App.js",
          "path": "${workspaceFolder}/src/App.js"
        }
      ]
    }
  ]
}
